  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "npm run dev:ssr",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "nx run website:serve-ssr",
    "serve:ssr": "node dist/website/server/main.js",
    "build:ssr": "nx build --prod && nx run website:server:production",
    "prerender": "nx run website:prerender",
    "prebuild:ssr": "ngcc",
    "sb:docs": "compodoc -p \"./tsconfig.base.json\" -e json -d \"./libs/ui-storybook/.storybook/public\"",
    "dc:copy:node-modules": "copyfiles \"./node_modules/**/*\" \"./tools/ci/dist\"",
    "dc:copy:dist": "copyfiles \"./dist/**/*\" \"./tools/ci\"",
    "dc:copy": "npm run dc:copy:node-modules && npm run dc:copy:dist",
    "sb:copy": "copyfiles \"./dist/storybook/**/*\" \"./docs-build\" --up 2",
    "serverless:copy": "npm run serverless:copy:core && npm run serverless:copy:functions && npm run serverless:copy:indexes",
    "serverless:copy:core": "copyfiles -f \"./tools/serverless/core/**/*\" \"./dist/apps/serverless\" --all",
    "serverless:copy:functions": "copyfiles \"./tools/serverless/functions/**/*\" \"./dist/apps/serverless\" --up 3",
    "serverless:copy:indexes": "copyfiles \"./tools/serverless/indexes/**/*\" \"./dist/apps/serverless/dist/\" --up 3",
    "serverless:build": "npm run serverless:copy && nx run serverless:build",
    "serverless:serve": "npm run serverless:build && func host start --script-root ../dark-rush-photography/dist/apps/serverless/",
    "serve:api": "concurrently \"nx serve api\" \"npm run serverless:serve\" \"nx serve web-socket\" \"nx run api-e2e:e2e --watch\"",
    "serve:ui": "npm run sb:docs && nx run ui-storybook-e2e:e2e --watch",
    "serve:web": "concurrently \"npm run dev:ssr\" \"npm run serve:api\" \"npm run serverless:serve\" \"nx serve web-socket\" \"nx run website-e2e:e2e --watch\"",
    "serve:best": "concurrently \"nx serve best-of\" \"nx serve api\" \"npm run serverless:serve\" \"nx serve web-socket\" \"nx run best-of-e2e:e2e --watch\""
  },
